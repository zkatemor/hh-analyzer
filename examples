CREATE TABLE hh_salary.vacancies
(`name` String,
`salary_from` Nullable(Float64),
`salary_to` Nullable(Float64),
`currency` String,
`salary_gross` Nullable(UInt8),
`experience` String,
`schedule` String,
`created_at` DateTime,
`employer` String,
`employment` String,
`city` String,
`area_url` String,
`area_id` String,
`area_name` String)
ENGINE = ReplacingMergeTree ORDER BY name